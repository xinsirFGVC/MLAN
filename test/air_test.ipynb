    "local_train = '/home/lvlab/Pytorch/object_air_train.npy'\n",
    "local_test = '/home/lvlab/Pytorch/object_air_test.npy'\n",
    "train_vectors = np.load(\"part_air_train.npy\")\n",
    "test_vectors = np.load(\"part_air_test.npy\")\n",
    "print(\"complete\")"
   ]
